import java.util.Scanner;

public class vacantionBook {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int numberPages = Integer.parseInt(scanner.nextLine());
        int pager = Integer.parseInt(scanner.nextLine());
        int days = Integer.parseInt(scanner.nextLine());

        int timePager = numberPages / pager;

        int timeForDay = timePager / days;

        System.out.println(timeForDay);
    }
}
